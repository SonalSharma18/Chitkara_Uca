#include<stdio.h>
#include<limits.h>
void main()
{
 printf("minimum value %d\n",INT_MIN);
 printf("maximum value %d \n",INT_MAX);
 printf("maximum value of short %d\n",SHRT_MAX);
 printf("minimum value of short %d \n",SHRT_MIN);
 printf("minimum size of long %d\n",SHRT_MIN);
 printf("maximum size of long %d\n",SHRT_MAX);
 printf("minimum size of long long %llu \n",LLONG_MIN);
 printf("maximum size of long long %llu \n",LLONG_MAX);
 printf("maximum size of unsigned is %lu \n",UINT_MAX);
 printf("maximum size of unsigned char %u \n",UCHAR_MAX);
 printf("maximum size of unsigned long %lu \n",ULONG_MAX);
}
