#include<stdio.h>
#include<stdlib.h>
void main()
{
	typedef int i;
 	i *a;
    i n,m;
 	printf("enter values of n and m");
 	scanf("%d%d",&n,&m);
 	a=(int*)malloc((n*m)*sizeof(int));
 	i *p=a;
 	i j,k;
 	for(j=0;j<n;j++)
 	{
       scanf("%d",&p[j]);
 	}
	for(j=0;j<n;j++)
 	{
       scanf("%d",&p[k]);
 	}
	printf("final\n");
	for(j=0;j<n*m;j++)
 	{
       printf("%d",p[j]);
	}
}
